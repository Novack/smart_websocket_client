{
  "manifest_version": 2,

  "name": "Smart websocket client",
  "description": "A tool to help you test websocket apis",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}